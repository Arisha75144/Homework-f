#include <iostream>     
using namespace std;
#include <vector>
struct info{
   unsigned int house_number = 0;
   string street_name = "Unknown";
    
    void PrintUserInfo() {
        cout << "street_name " << street_name;
        cout<<"house_number"<<house_number;
        
    }
}
int main() {

    info user;
    string x;
    int y;
    cin>>x;
    cin>>y;
    user.street_name = x;
    user.house_number = y;
    user.PrintUserInfo();
}
